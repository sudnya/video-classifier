#! /usr/bin/python

################################################################################
#    \file   SConscript
#    \author Gregory Diamos <solusstultus@gmail.com>
#    \date   June 13, 2015
#    \brief  The SCons program build script for lucious benchmarks
################################################################################


Import('env')

lucious_libs = env['LUCIOUS_LIBS']

programs = []

programs.append(env.Program('benchmark-imagenet', \
    ['image-net/benchmark-image-net.cpp'], LIBS=lucious_libs))

Return('programs')


